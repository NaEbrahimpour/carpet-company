
package carpet_company;

import java.io.FileReader;

public class get_image_seq {
    static int A[][] = new int [30][40];
    public static void funct(String t) throws  Exception{
        FileReader fr = new FileReader(t);
         
         for(int i=0 ; i<30 ; i++)
            for(int j=0 ; j<40 ; j++){
                A[i][j] = fr.read();
               
            }
    }
    
}
