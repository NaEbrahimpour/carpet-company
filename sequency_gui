    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        int gen1[] = new int[1200];
        int gen2[] = new int[1200];
        int gen3[] = new int[1200];
        int gen4[] = new int[1200];
        int gen5[] = new int[1200];
        int gen6[] = new int[1200];
        make_pic m = new make_pic();
        get_image_seq g = new get_image_seq();
        int araay[] = new int[5];
        try {
            m.func();
        } catch (IOException ex) {
            Logger.getLogger(seqency.class.getName()).log(Level.SEVERE, null, ex);
        }
        
            gen1 = m.two_one(m.pix1);
            gen2 = m.two_one(m.pix2);
            gen3 = m.two_one(m.pix3);
            gen4 = m.two_one(m.pix4);
            gen5 = m.two_one(m.pix5);
        try {
            get_image_seq.funct(jTextField1.getText());
        }
         catch (Exception ex) {
            Logger.getLogger(seqency.class.getName()).log(Level.SEVERE, null, ex);
        }
        gen6 = m.two_one(g.A);
      
         araay[0] = sequence.getMinimumPenalty(gen6, gen1, 1, 2);
         araay[1] = sequence.getMinimumPenalty(gen6, gen2, 1, 2);
         araay[2] = sequence.getMinimumPenalty(gen6, gen3, 1, 2);
         araay[3] = sequence.getMinimumPenalty(gen6, gen4, 1, 2);
         araay[4] = sequence.getMinimumPenalty(gen6, gen5, 1, 2);
         for(int i=0 ; i<5 ; i++)
             System.out.println(araay[i]);
        
         
        
    }                                        

